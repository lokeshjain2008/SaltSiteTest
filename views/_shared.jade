					button(class=["navbar-toggle", "collapsed"], data-toggle="collapse", aria-expanded="false", data-target="#navigation")
						span.sr-only Tooglenavigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand(href="/") Home
				.collapse.navbar-collapse#navigation
					ul.nav.navbar-nav
						li(class={active: path==='/services'})
							a(href='/services') Services
						li(class={active: path==='/models'})
							a(href='/models') Models
						li(class={active: path==='/reviews'})
							a(href='/reviews') Reviews
		.container
			block body
			script(src="js/app.js")
				